[ignore]
.*\/node_modules\/.*\/fbjs\/.*
.*\/node_modules\/.*\/invariant\/.*
.*\/node_modules\/koa-static\/.*

[include]
./src/
./data/
./config/

[libs]
./interfaces/

[options]
esproposal.class_instance_fields=ignore
esproposal.class_static_fields=ignore
esproposal.decorators=ignore

module.name_mapper='^.*\.css$' -> 'empty/object'
module.name_mapper='^actions\/\(.*\)$' -> '<PROJECT_ROOT>/src/actions/\1'
module.name_mapper='^actions$' -> '<PROJECT_ROOT>/src/actions'
module.name_mapper='^components\/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^components$' -> '<PROJECT_ROOT>/src/components'
module.name_mapper='^constants\/\(.*\)$' -> '<PROJECT_ROOT>/src/constants/\1'
module.name_mapper='^constants$' -> '<PROJECT_ROOT>/src/constants'
module.name_mapper='^containers\/\(.*\)$' -> '<PROJECT_ROOT>/src/containers/\1'
module.name_mapper='^containers$' -> '<PROJECT_ROOT>/src/containers'
module.name_mapper='^entryPoints\/\(.*\)$' -> '<PROJECT_ROOT>/src/entryPoints/\1'
module.name_mapper='^entryPoints$' -> '<PROJECT_ROOT>/src/entryPoints'
module.name_mapper='^reducers\/\(.*\)$' -> '<PROJECT_ROOT>/src/reducers/\1'
module.name_mapper='^reducers$' -> '<PROJECT_ROOT>/src/reducers'
module.name_mapper='^routes\/\(.*\)$' -> '<PROJECT_ROOT>/src/routes/\1'
module.name_mapper='^routes$' -> '<PROJECT_ROOT>/src/routes'
module.name_mapper='^store\/\(.*\)$' -> '<PROJECT_ROOT>/src/store/\1'
module.name_mapper='^store$' -> '<PROJECT_ROOT>/src/store'
module.name_mapper='^utils\/\(.*\)$' -> '<PROJECT_ROOT>/src/utils/\1'
module.name_mapper='^utils$' -> '<PROJECT_ROOT>/src/utils'
module.name_mapper='^views\/\(.*\)$' -> '<PROJECT_ROOT>/src/views/\1'
module.name_mapper='^views$' -> '<PROJECT_ROOT>/src/views'
module.name_mapper='^config\/\(.*\)$' -> '<PROJECT_ROOT>/config/\1'

module.system=haste

munge_underscores=true

strip_root=true

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe\\($\\|[^(]\\|(\\(>=0\\.\\(1[0-6]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue\\((\\(>=0\\.\\(1[0-6]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)? #[0-9]+
suppress_comment=\\(.\\|\n\\)*\\$FlowFixedInNextDeploy

[version]
0.18.1
